# ESTA ES LA GRÁFICA QUE NO TIENE SENTIDO
plot(density(retornos$SP),
     col="red",
     ylim=c(0,60), 
     main="Distribuciones contra normal")+
  lines(density(retornos$OMX30),
     col="green",
     ylim=c(0,60))+
